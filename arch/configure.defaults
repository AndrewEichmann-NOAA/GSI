#ARCH	Linux i486 i586 i686, gfortran compiler # dmpar 
#
LDFLAGS         =      -Wl,-noinhibit-exec

COREDIR = CONFIGURE_CORE_PATH
INC_DIR = $(COREDIR)/include
BYTE_ORDER=LITTLE_ENDIAN
SFC=gfortran
SF90=gfortran -ffree-form
SCC=gcc
INC_FLAGS = -qmoddir=$(INC_DIR) -I $(INC_DIR)
FFLAGS=$(INC_FLAGS) -fconvert=big-endian -fno-second-underscore -fno-range-check -frecord-marker=4 $(LDFLAGS)
CPP             =      /lib/cpp 
CPPFLAGS       =      -C -P -D$(BYTE_ORDER) -DWRF

DM_FC=gfortran
DM_F90=gfortran -ffree-form
DM_CC=gcc

FC              =       CONFIGURE_FC
F90             =       CONFIGURE_F90
CC              =       CONFIGURE_CC

CFLAGS=-O0 -DLINUX -D_OPENMP

