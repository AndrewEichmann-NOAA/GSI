#ARCH	Linux i486 i586 i686, Intel compiler	# dmpar 
#
LDFLAGS         =     -Wl,-noinhibit-exec 

COREDIR = CONFIGURE_CORE_PATH
INC_DIR = $(COREDIR)/include
BYTE_ORDER=LITTLE_ENDIAN
SFC=ifort
SF90=ifort -free
SCC=icc
INC_FLAGS = -module $(INC_DIR) -I $(INC_DIR)
FFLAGS=-O3 -xT $(INC_FLAGS) -fp-model precise -assume byterecl -convert big_endian -fpe0 -g -traceback $(LDFLAGS)
CPP             =      /lib/cpp 
CPPFLAGS       =      -C -P -D$(BYTE_ORDER) -DWRF

DM_FC=ifort
DM_F90=ifort -free
DM_CC=icc

FC              =       CONFIGURE_FC
F90             =       CONFIGURE_F90
CC              =       CONFIGURE_CC

CFLAGS=-O0 -DLINUX


