FFLAGS_SINGLE  =   -i4 -r4
CPP             =      /lib/cpp 
CPPFLAGS       =      -C -P -D$(BYTE_ORDER) -DWRF

DM_FC=mpif90 -f90=pgf90
DM_F90=mpif90 -Mfree -f90=pgf90
DM_CC=mpicc 

FC              =       CONFIGURE_FC
F90             =       CONFIGURE_F90
CC              =       CONFIGURE_CC

CFLAGS=-O0 -DLINUX -DUNDERSCORE
CFLAGS2=-DLINUX -Dfunder -DFortranByte=char -DFortranInt=int -DFortranLlong='long long'


MYLIBsys          =  -llapack -lblas
NETCDFLIBS_2= -rpath,CONFIGURE_NETCDF_PATH/lib

###########################################################
#ARCH	Linux i486 i586 i686, Intel compiler	# dmpar 
#
LDFLAGS         =     -Wl,-noinhibit-exec 

COREDIR = CONFIGURE_CORE_PATH
INC_DIR = $(COREDIR)/include
BYTE_ORDER=LITTLE_ENDIAN
SFC=ifort
SF90=ifort -free
SCC=icc
INC_FLAGS = -module $(INC_DIR) -I $(INC_DIR)
FFLAGS=-O3 -xW $(INC_FLAGS) -fp-model precise -assume byterecl -convert big_endian -fpe0 -g -traceback $(LDFLAGS) -DLINUX
FFLAGS_DOUBLE  =   -i4 -r8
FFLAGS_SINGLE  =   -i4 -r4
CPP             =      /lib/cpp 
CPPFLAGS       =      -C -P -D$(BYTE_ORDER) -DWRF

DM_FC=ifort
DM_F90=mpif90 -free
DM_CC=cc

FC              =       CONFIGURE_FC
F90             =       CONFIGURE_F90
CC              =       CONFIGURE_CC

CFLAGS=-O0 -DLINUX -DUNDERSCORE
CFLAGS2=-DLINUX -Dfunder -DFortranByte=char -DFortranInt=int -DFortranLlong='long long'

MYLIBsys          = -L$(LAPACK_PATH) -lmkl_lapack -lmkl_em64t -lguide -lpthread -lm
NETCDFLIBS_2= 

